{
	"name": "garrett.culos.ts.node",
	"version": "0.0.1",
	"description": "A starting point for Node.js express apps with TypeScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/TypeScript-Node-Starter"
	},
	"author": "Garrett Culos",
	"license": "MIT",
	"scripts": {
		"cr": "npm run build && npm run test",
		"copy-static-assets": "ts-node copyStaticAssets.ts",
		"build.compile": "tsc",
		"build": "npm run pre.build && npm run build.compile && npm run tslint && npm run copy-static-assets",
		"debug": "npm run build && npm run watch-debug",
		"format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss}\"",
		"format:fix": "prettier --config ./.prettierrc --staged --write \"src/**/*\"",
		"init": "npm cr && npm run start",
		"precommit": "npm run format:fix && npm run tslint",
		"pre.build": "rm -rf dist/",
		"serve": "node dist/server.js",
		"seed": "node_modules/.bin/sequelize db:seed:all",
		"seed.dev": "node_modules/.bin/sequelize db:seed:all",
		"seed.dev.undo": "node_modules/.bin/sequelize db:seed:undo:all",
		"serve-debug": "nodemon --inspect dist/server.js",
		"start": "npm run serve",
		"test": "jest --forceExit --coverage --verbose",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
		"watch-ts": "tsc -w",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
	},
	"dependencies": {
		"@types/async": "^2.0.49",
		"@types/bcrypt-nodejs": "^0.0.30",
		"@types/bluebird": "^3.5.23",
		"@types/body-parser": "^1.17.0",
		"@types/cheerio": "^0.22.8",
		"@types/compression": "^0.0.35",
		"@types/cron": "^1.3.0",
		"@types/dotenv": "^4.0.3",
		"@types/errorhandler": "^0.0.32",
		"@types/express": "^4.16.0",
		"@types/express-session": "^1.15.10",
		"@types/glob": "^5.0.35",
		"@types/jest": "^22.2.3",
		"@types/jquery": "^3.3.6",
		"@types/lodash": "^4.14.116",
		"@types/lusca": "^1.5.0",
		"@types/mime": "^2.0.0",
		"@types/morgan": "^1.7.35",
		"@types/mysql": "^2.15.5",
		"@types/node": "^9.6.28",
		"@types/nodemailer": "^4.6.2",
		"@types/request": "^2.47.1",
		"@types/sequelize": "^4.27.24",
		"@types/serve-static": "^1.13.2",
		"@types/shelljs": "^0.7.9",
		"@types/supertest": "^2.0.5",
		"@types/winston": "^2.4.4",
		"async": "^2.6.1",
		"bcrypt-nodejs": "^0.0.3",
		"bluebird": "^3.5.1",
		"body-parser": "^1.18.3",
		"capistrano": "^1.0.0",
		"chai": "^4.1.2",
		"cheerio": "^1.0.0-rc.2",
		"compression": "^1.7.3",
		"concurrently": "^3.6.1",
		"cron": "^1.4.0",
		"dotenv": "^4.0.0",
		"errorhandler": "^1.5.0",
		"express": "^4.16.3",
		"express-flash": "0.0.2",
		"express-session": "^1.15.6",
		"express-validator": "^4.3.0",
		"jest": "^22.4.4",
		"lodash": "^4.17.10",
		"lusca": "^1.6.1",
		"mime": "^2.3.1",
		"minimatch": "^3.0.4",
		"mysql": "^2.16.0",
		"mysql2": "^1.6.1",
		"node-sass": "^4.9.3",
		"nodemailer": "^4.6.8",
		"nodemon": "^1.18.3",
		"request": "^2.88.0",
		"request-promise": "^4.2.2",
		"sequelize": "^4.38.0",
		"sequelize-cli": "^4.1.0",
		"shelljs": "^0.8.2",
		"supertest": "^3.1.0",
		"ts-jest": "^22.4.6",
		"ts-node": "^5.0.1",
		"tslib": "^1.9.3",
		"tslint": "^5.11.0",
		"typescript": "^3.0.1",
		"winston": "^2.4.3"
	}
}
