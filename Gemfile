# frozen_string_literal: true

source "https://rubygems.org"

# gem "rails"
group :development do
    gem "capistrano", "~> 3.10", require: false

    # integrate bundler with capistrano
    gem 'capistrano-bundler'

    # if you are using RBENV
    gem 'capistrano-ext'

    gem 'capistrano-npm'
end