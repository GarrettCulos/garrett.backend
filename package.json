{
	"name": "express-typescript-starter",
	"version": "0.1.0",
	"description": "A starting point for Node.js express apps with TypeScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/TypeScript-Node-Starter"
	},
	"author": "Bowden Kelly",
	"license": "MIT",
	"scripts": {
		"start": "npm run serve",
		"build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
		"serve": "node dist/server.js",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"test": "jest --forceExit --coverage --verbose",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss}\"",
		"format:fix": "prettier --config ./.prettierrc --staged --write \"src/**/*\"",
		"precommit": "npm run format:fix && npm run tslint",
		"copy-static-assets": "ts-node copyStaticAssets.ts",
		"debug": "npm run build && npm run watch-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
		"cr": "npm run build && npm run test"
	},
	"dependencies": {
		"@types/async": "^2.0.49",
		"@types/bcrypt-nodejs": "^0.0.30",
		"@types/bluebird": "^3.5.20",
		"@types/body-parser": "^1.16.8",
		"@types/cheerio": "^0.22.7",
		"@types/compression": "^0.0.35",
		"@types/cron": "^1.3.0",
		"@types/dotenv": "^4.0.3",
		"@types/errorhandler": "^0.0.32",
		"@types/express": "^4.11.1",
		"@types/express-session": "^1.15.8",
		"@types/glob": "^5.0.35",
		"@types/jest": "^22.2.3",
		"@types/jquery": "^3.3.1",
		"@types/lodash": "^4.14.106",
		"@types/lusca": "^1.5.0",
		"@types/mime": "^2.0.0",
		"@types/morgan": "^1.7.35",
		"@types/mysql": "^2.15.4",
		"@types/node": "^9.6.4",
		"@types/nodemailer": "^4.6.0",
		"@types/request": "^2.47.0",
		"@types/sequelize": "^4.27.14",
		"@types/serve-static": "^1.13.1",
		"@types/shelljs": "^0.7.8",
		"@types/supertest": "^2.0.4",
		"@types/winston": "^2.3.9",
		"async": "^2.6.0",
		"bcrypt-nodejs": "^0.0.3",
		"bluebird": "^3.5.1",
		"body-parser": "^1.18.2",
		"capistrano": "^1.0.0",
		"chai": "^4.1.2",
		"cheerio": "^1.0.0-rc.2",
		"compression": "^1.7.2",
		"concurrently": "^3.5.1",
		"cron": "^1.3.0",
		"dotenv": "^4.0.0",
		"errorhandler": "^1.5.0",
		"express": "^4.16.3",
		"express-flash": "0.0.2",
		"express-session": "^1.15.6",
		"express-validator": "^4.3.0",
		"jest": "^22.4.3",
		"lodash": "^4.17.5",
		"lusca": "^1.6.0",
		"mime": "^2.3.1",
		"minimatch": "^3.0.4",
		"mysql": "^2.15.0",
		"mysql2": "^1.5.3",
		"node-sass": "^4.9.2",
		"nodemailer": "^4.6.4",
		"nodemon": "^1.18.3",
		"request": "^2.85.0",
		"request-promise": "^4.2.2",
		"sequelize": "^4.37.6",
		"shelljs": "^0.8.1",
		"supertest": "^3.0.0",
		"ts-jest": "^22.4.2",
		"ts-node": "^5.0.1",
		"tslib": "^1.9.0",
		"tslint": "^5.9.1",
		"typescript": "^2.8.1",
		"winston": "^2.4.1"
	}
}
